{
  "app": {
    "bundleName": "xwg.harmony.stopwatch",
    "vendor": "harmony",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Release"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "xwg.harmony.stopwatch",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home",
              "action.setting"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "name": "xwg.harmony.stopwatch.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "permissions": [
          "ohos.permission.KEEP_BACKGROUND_RUNNING"
        ]
      },
      {
        "backgroundModes": [
          "dataTransfer",
          "audioPlayback"
        ],
        "name": "xwg.harmony.stopwatch.StopWatchService",
        "icon": "$media:icon",
        "description": "$string:stopwatchservice_description",
        "type": "service"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.KEEP_BACKGROUND_RUNNING",
        "reason": "$string:permreason_keepbackround",
        "usedScene": {
          "ability": [
            "xwg.harmony.stopwatch.MainAbility"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.LOCATION",
        "reason": "$string:reason_location",
        "usedScene": {
          "ability": [
            "com.myapplication.LocationAbility"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.INTERNET"
      }
    ]
  }
}