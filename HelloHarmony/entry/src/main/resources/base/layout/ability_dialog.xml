<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical">
    <Button
        ohos:id="$+id:std_common_dialog"
        ohos:width="match_parent"
        ohos:height="match_content"
        ohos:text_size="20fp"
        ohos:text="CommonDialog"
        ohos:text_color="#FFFFFF"
        ohos:layout_alignment="center"
        ohos:background_element="$graphic:background_button"
        ohos:right_padding="5vp"
        ohos:left_padding="5vp"
        ohos:margin="5vp"
        />
    <Button
        ohos:id="$+id:customize_common_dialog"
        ohos:width="match_parent"
        ohos:height="match_content"
        ohos:text_size="20fp"
        ohos:text="CustomizeCommonDialog"
        ohos:text_color="#FFFFFF"
        ohos:layout_alignment="center"
        ohos:background_element="$graphic:background_button"
        ohos:right_padding="5vp"
        ohos:left_padding="5vp"
        ohos:margin="5vp"
        />
    <Button
        ohos:id="$+id:list_dialog"
        ohos:width="match_parent"
        ohos:height="match_content"
        ohos:text_size="20fp"
        ohos:text="ListDialog"
        ohos:text_color="#FFFFFF"
        ohos:layout_alignment="center"
        ohos:background_element="$graphic:background_button"
        ohos:right_padding="5vp"
        ohos:left_padding="5vp"
        ohos:margin="5vp"
        />
    <Button
        ohos:id="$+id:customized_list_dlg"
        ohos:width="match_parent"
        ohos:height="match_content"
        ohos:text_size="20fp"
        ohos:text="CustomizedListDialog"
        ohos:text_color="#FFFFFF"
        ohos:layout_alignment="center"
        ohos:background_element="$graphic:background_button"
        ohos:right_padding="5vp"
        ohos:left_padding="5vp"
        ohos:margin="5vp"
        />
</DirectionalLayout>