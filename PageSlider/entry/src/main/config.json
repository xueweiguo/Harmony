{
  "app": {
    "bundleName": "com.components.pageslider",
    "vendor": "components",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Beta1"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.components.pageslider",
    "name": ".MyApplication",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry"
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "name": "com.components.pageslider.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "PageSlider",
        "type": "page",
        "launchType": "standard"
      }
    ]
  },
  "reqPermissions": [
    {
      "name": "ohos.permission.SENSOR_TYPE_SCREEN_ROTATION",
      "reason": "",
      "usedScene": {
        "ability": [
          ".MainAbility"
        ],
        "when": "inuse"
      }
    }
  ]
}